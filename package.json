{
  "name": "i18next",
  "version": "20.0.0",
  "description": "",
  "type": "module",
  "main": "./dist/cjs/i18next.js",
  "module": "./dist/esm/i18next.js",
  "browser": "./dist/umd/i18next.js",
  "types": "./index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/cjs/i18next.js",
      "default": "./dist/esm/i18next.js"
    },
    "./cjs": {
      "default": "./dist/cjs/i18next.js"
    },
    "./esm": {
      "default": "./dist/esm/i18next.js"
    }
  },
  "scripts": {
    "lint:javascript": "eslint .",
    "lint:typescript": "eslint -c types/.eslintrc types/**/*.d.ts test/types/**/*.test-d.ts",
    "lint": "npm run lint:javascript && npm run lint:typescript",
    "build": "rm -rf dist && rollup -c && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "test:deno": "deno test test/deno/*.ts --reload",
    "test:typescript": "tsd",
    "test": "npm run lint && mocha --colors --reporter spec --recursive test/*.js",
    "test:all": "npm run test && npm run test:typescript && npm run test:deno"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@types/mocha": "8.0.3",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "babel-plugin-add-module-exports": "1.0.4",
    "eslint": "7.10.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-require-path-exists": "1.1.9",
    "eslint-plugin-standard": "4.0.1",
    "mocha": "8.1.3",
    "rollup": "2.28.2",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "7.0.2",
    "should": "13.2.3",
    "tsd": "0.13.1",
    "typescript": "4.0.3"
  },
  "tsd": {
    "directory": "test/types"
  }
}
